# Server Configuration
NODE_ENV=development
PORT=3001
HOST=0.0.0.0

# CORS Configuration
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,https://app.5glabx.com

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/5glabx_cloud
REDIS_URL=redis://localhost:6379

# Log Engine Configuration
LOG_ENGINE_URL=http://localhost:3001
GLOBAL_FAULT_RATE=0.10

# Security
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_CONNECTION_TIMEOUT=300000

# Test Execution
MAX_CONCURRENT_EXECUTIONS=10
EXECUTION_TIMEOUT=300000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Feature Flags
ENABLE_FAULT_INJECTION=true
ENABLE_REAL_TIME_COLLABORATION=true
ENABLE_ADMIN_PANEL=true
ENABLE_ANALYTICS=true

# Development
DEBUG_MODE=true
MOCK_DATA=true