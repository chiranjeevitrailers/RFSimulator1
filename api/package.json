    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "tsx": "^3.12.7",
    "vitest": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "@vitest/coverage-v8": "^0.34.0"
